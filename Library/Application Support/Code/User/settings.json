{
    "vs-kubernetes": {
        "vscode-kubernetes.minikube-path.mac": "/Users/jason/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "redhat.telemetry.enabled": false,
    "editor.fontSize": 14,
    "editor.tabSize": 2,
    "vim.hlsearch": true,
    "vim.joinspaces": false,
    "vim.leader": " ",
    "vim.changeWordIncludesWhitespace": false,
    "vim.useSystemClipboard": true,
    "vim.visualModeKeyBindings": [
    
    ],
    "window.zoomLevel": 2,
    "vim.normalModeKeyBindings": [
        // Bookmarks
        {
            "before": [ "<leader>", "m" ],
            "commands": [ "bookmarks.toggle" ]
        },
        {
            "before": [ "<leader>", "b" ],
            "commands": [ "bookmarks.list" ]
        },
        // Indent/outdent
        {
            "before": [ ">" ],
            "commands": [ "editor.action.indentLines" ]
        },
        {
            "before": [ "<" ],
            "commands": [ "editor.action.outdentLines" ]
        },
        // Problems
        {
            "before": [ "<leader>", "J" ],
            "commands": [ "workbench.actions.view.problems" ]
        },
        {
            "before": [ "<leader>", "j" ],
            "commands": [ "editor.action.marker.next" ]
        },
        {
            "before": [ "<leader>", "k" ],
            "commands": [ "editor.action.marker.prev" ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [ "=" ],
            "after": [ "" ],
            "commands": [ "editor.action.reindentlines" ]
        }
    ],
    "vim.gdefault": true,
    "vim.incsearch": true,
    "vim.textwidth": 76,
    "editor.renderFinalNewline": "off",
    "editor.rulers": [ 80, 100, 120 ],
    "editor.autoClosingBrackets": "never",
    "editor.autoSurround": "never",
    "editor.autoClosingQuotes": "never",
    "html.autoClosingTags": false,
    "editor.minimap.enabled": false,
    "vsicons.dontShowNewVersionMessage": true,
    "window.clickThroughInactive": false,
    "editor.cursorBlinking": "expand",
    "platformio-ide.activateOnlyOnPlatformIOProject": true,
    "platformio-ide.forceUploadAndMonitor": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
    "debug.terminal.clearBeforeReusing": true,
    "html.autoCreateQuotes": false,
    "vim.commandLineModeKeyBindingsNonRecursive": [
    ],
    "remote.SSH.defaultForwardedPorts": [],
    "[python]": {
        "editor.formatOnType": true
    },
    "prisma.showPrismaDataPlatformNotification": false,
    "python.defaultInterpreterPath": "/Users/jason/swiper/.direnv/python-3.12",
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.callArgumentNames": "partial",
    "python.experiments.enabled": false,
    "python.analysis.autoFormatStrings": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.testing.pytestArgs": [
        "-rP"
    ]
}